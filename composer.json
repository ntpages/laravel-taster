{
    "name": "ntpages/laravel-taster",
    "description": "Laravel package for A/B testing & feature staged rollouts",
    "keywords": [
        "laravel",
        "ab-testing",
        "features",
        "rollouts"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Stasyuk",
            "email": "anton@ntpages.com.au"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.7.25"
    },
    "autoload": {
        "psr-4": {
            "Ntpages\\LaravelTaster\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ntpages\\LaravelTaster\\Provider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": []
    }
}
