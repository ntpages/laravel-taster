{
    "name": "ntpages/laravel-taster",
    "description": "Laravel package for A/B testing",
    "keywords": [
        "laravel",
        "ab-testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Stasyuk",
            "email": "anton@ntpages.com.au"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "laravel/framework": "^8.x"
    },
    "autoload": {
        "psr-4": {
            "Ntpages\\LaravelTaster\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ntpages\\LaravelTaster\\Provider"
            ]
        }
    }
}
